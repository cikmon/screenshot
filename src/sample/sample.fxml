<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.Controller"
            styleClass="background">


   <children>
      <ImageView fx:id="mainImageView" fitHeight="424.0" fitWidth="561.0" layoutX="107.0"
                 layoutY="12.0" pickOnBounds="true" preserveRatio="true"
                 AnchorPane.topAnchor="5"
                 AnchorPane.bottomAnchor="158.0"
                 AnchorPane.leftAnchor="5.0"
                 AnchorPane.rightAnchor="0.0"

      />

       <Button defaultButton="true"
               fx:id="stopRun" layoutX="726.0" layoutY="12.0" AnchorPane.rightAnchor="5.0"
               mnemonicParsing="false" onAction="#stopRun" styleClass="default" text="Run" />

       <Button layoutX="688.0" layoutY="44.0" AnchorPane.rightAnchor="5.0"
              mnemonicParsing="false" onAction="#runn" text="Load images" />

      <ScrollPane fx:id="scrollPane" fitToWidth="true" layoutX="-4.0" layoutY="441.0" maxWidth="1.7976931348623157E308"
                  prefHeight="158.0" prefWidth="804.0"
                  styleClass="background"
                  AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0"
      >
         <content>
            <HBox fx:id="imagesStore" prefHeight="134.0" prefWidth="143.0" />
         </content>
      </ScrollPane>
<!--      <Button layoutX="698.0" layoutY="173.0" mnemonicParsing="false" onAction="#saveToBuff" text="Button" />-->
<!--      <Button layoutX="700.0" layoutY="237.0" mnemonicParsing="false" onAction="#seveBufScreen" text="seveBufScreen" />-->
   </children>
</AnchorPane>
